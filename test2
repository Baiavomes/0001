4
let nacimento = 2008;
let ano_atual = 2023;
let idade =(nacimento-ano_atual);
let mes = (idade*12);
let diaas =(idade*365);
let semananas = (idade*52.143);
console.log("você ta vivo a", idade,"anos", mes,"meses", semananas,"semanas", diaas, "dias");

